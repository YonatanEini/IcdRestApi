{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "InitilaizedIcdData": {
    "IcdFilesPath": "C:\\ICDResources\\",
    "CommunicationIcdDict": {
      "FlightBoxUp": [
        "FlightBoxDownICD",
        "FlightBoxUpICD"
      ],
      "FlightBoxDown": [
        "FlightBoxDownICD",
        "FlightBoxUpICD"
      ],
      "FiberBoxDown": [
        "FlightBoxDownICD",
        "FlightBoxUpICD"
      ],
      "FiberBoxUp": [
        "FlightBoxDownICD",
        "FlightBoxUpICD"
      ],
      "LandingBox": [
        "FlightBoxDownICD",
        "FlightBoxUpICD"
      ]
    },
    "DestinationPath": "C:\\ClientIcdFiles\\"
  },
  "producer": {
    "BootstrapServers": "127.0.0.1:9092",
    "ClientId": "Dns.GetHostName()",
    "EnableDeliveryReports": true,
    "MessageSendMaxRetries": 3,
    "RetryBackoffMs": 1000,
    "EnableIdempotence": true,
    "MessageTimeoutMs": 5000
  },
  "consumer": {
    "BootstrapServers": "127.0.0.1:9092",
    "GroupId": "gid-consumers"
  },
  "AllowedHosts": "*"
}
